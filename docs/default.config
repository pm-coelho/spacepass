[PasswordStore]
; Path for the password-store repository
; defaults to $HOME.password-store
;pass-dir <pass-path>

[Shape]
inner-margin 10
column-spacing 1
row-spacing 1
max-columns 6
row-height 35
;font italic bold 12px Georgia, serif

[Colors]
; All colors default to the GTK theme in use
; values should be hex color codes withouth the hashtag(#)
background-color cc241d
button-background-color cc241d
button-text-color 263238
